<h1>Edit Contact:</h1>
<%= form_tag "/contacts/#{@contact.id}", method: :patch do %>
  <div class="form-group">
    First Name: <input type="text" class="form-control" value="<%= @contact.first_name %>" name="first_name">
  </div>
  <div class="form-group">
    Middle Name: <input type="text" class="form-control" value="<%= @contact.middle_name %>" name="middle_name">
  </div>
  <div class="form-group">
    Last Name: <input type="text" class="form-control" value="<%= @contact.last_name %>" name="last_name">
  </div>
  <div class="form-group">
    Email Address: <input type="text" class="form-control" value="<%= @contact.email %>" name="email">
  </div>
  <div class="form-group">
    Phone Number: <input type="text" class="form-control" value="<%= @contact.phone_number %>" name="phone_number">
    </div>
  <input type="submit" class="btn btn-default" value="Update contact">
<% end %>


